            "intervals" : [ 5, 6, 7 ],
            "min" : 0,
            "divideBy" : "vmCount"
          }, {
            "label" : "Disk Response (s)",
            "id" : "host.diskTest",
            "intervals" : [ 3, 5, 10 ],
            "min" : 0,
            "divideBy" : "vmCount"
          }, {
            "label" : "CsAgent Cpu (Total)",
            "id" : "os-process.topCpu_CsAgent",
            "intervals" : [ 10, 30, 100 ]
          } ]
