            "report" : "os-detail/detail",
            "metric" : "socketCount",
            "serviceName" : "CsAgent",
            "divideBy" : "numberOfSamples"
          }, {
            "label" : "CsAgent OS Commands",
            "report" : "application/detail",
            "metric" : "OsCommandsCounter",
            "serviceName" : "CsAgent",
            "divideBy" : "numberOfSamples"
          } ],
