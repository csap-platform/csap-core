				}
			},
			"metaData": "exportWeb, -nio",
			"servletMaxConnections": "400",
			"servletTimeoutMs": "10000",
			"servletThreadCount": "100",
			"monitors": {
				"max_diskUtil": "3000",
				"max_threadCount": "300",
				"max_fileCount": "489",
				"max_rssMemory": "768000",
				"max_tomcatConnections": "20",
				"max_topCpu": "150"
